	; MsgBox, String Input is %string_input%
    if (string_input in alt_code_map)
        return alt_code_map[string_input]
    else
        return ""
}

ListenToNumpads:
    Hotkey, Numpad0, AppendNumpadInput, On
    Hotkey, Numpad1, AppendNumpadInput, On
    Hotkey, Numpad2, AppendNumpadInput, On
    Hotkey, Numpad3, AppendNumpadInput, On
    Hotkey, Numpad4, AppendNumpadInput, On
    Hotkey, Numpad5, AppendNumpadInput, On
    Hotkey, Numpad6, AppendNumpadInput, On
    Hotkey, Numpad7, AppendNumpadInput, On
    Hotkey, Numpad8, AppendNumpadInput, On
    Hotkey, Numpad9, AppendNumpadInput, On
	KeyWait, LAlt, D
	return

; Listen for LAlt press up
StopListeningToNumpads:
    Hotkey, Numpad0, AppendNumpadInput, Off
    Hotkey, Numpad1, AppendNumpadInput, Off
    Hotkey, Numpad2, AppendNumpadInput, Off
    Hotkey, Numpad3, AppendNumpadInput, Off
    Hotkey, Numpad4, AppendNumpadInput, Off
    Hotkey, Numpad5, AppendNumpadInput, Off
    Hotkey, Numpad6, AppendNumpadInput, Off
    Hotkey, Numpad7, AppendNumpadInput, Off
    Hotkey, Numpad8, AppendNumpadInput, Off
    Hotkey, Numpad9, AppendNumpadInput, Off

	; ToolTip, Numpad Input is %numpad_input%

    if (numpad_input != "")
    {
		numpad_input_string := numpad_input
		char := ConvertAltCodes(numpad_input_string)
		; MsgBox, Char is %char%
        if (char != "")
        {
            Send, {%char%}
        }
		numpad_input := ""
    }
	return

AppendNumpadInput:
    key := SubStr(A_ThisHotkey, 7)
    numpad_input .= "" key
	return
	